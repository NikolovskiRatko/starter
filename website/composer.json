{
  "name": "starter",
  "description": "A Single Page Application boilerplate using  Laravel 5.7, Vue.js 2.5, Bootstrap 4, TypeScript and Sass",
  "keywords": [
    "laravel",
    "vue",
    "boilerplate",
    "typescript",
    "sass",
    "scss",
    "pug",
    "jest"
  ],
  "authors": [
    {
      "name": "Ratko Nikolovski",
      "email": "nikolovski.ratko@gmail.com",
      "homepage": "https://nikolovski.org"
    },
    {
      "name": "The Laravel Framework",
      "homepage": "https://laravel.com"
    },
    {
      "name": "The Vue Framework",
      "homepage": "https://vuejs.org/"
    },
    {
      "name": "Bootstrap",
      "homepage": "https://getbootstrap.com/"
    }
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": "^7.1.3",
    "ext-json": "*",
    "anthonymartin/geo-location": "^1.0",
    "barryvdh/laravel-debugbar": "^3.2.3",
    "dimsav/laravel-translatable": "9.*",
    "fideloper/proxy": "^4.0",
    "guzzlehttp/guzzle": "^6.3",
    "laravel/framework": "5.7.*",
    "laravel/tinker": "^1.0",
    "martinlindhe/laravel-vue-i18n-generator": "^0.1.20",
    "nutsweb/laravel-prerender": "^2.0",
    "pda/pheanstalk": "~3.0",
    "predis/predis": "^1.1",
    "propaganistas/laravel-phone": "^4.2",
    "spatie/geocoder": "^3.4",
    "spatie/laravel-medialibrary": "^7.0.0",
    "staudenmeir/belongs-to-through": "^2.5",
    "torann/geoip": "^1.0",
    "tymon/jwt-auth": "1.0.0-rc.3",
    "webpatser/laravel-countries": "dev-master",
    "zizaco/entrust": "5.2.x-dev"
  },
  "require-dev": {
    "barryvdh/laravel-ide-helper": "^2.6",
    "beyondcode/laravel-dump-server": "^1.0",
    "filp/whoops": "^2.0",
    "fzaninotto/faker": "^1.9",
    "mockery/mockery": "^1.0",
    "nunomaduro/collision": "^2.0",
    "nunomaduro/larastan": "^0.4.0",
    "phpunit/phpunit": "^7.0"
  },
  "autoload": {
    "classmap": ["database/seeds", "database/factories"],
    "psr-4": {
      "App\\": "app/"
    },
    "files": ["app/Http/helpers.php"]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/php/"
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": ["@php artisan key:generate"],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ],
    "test": "\"vendor/bin/phpunit\"",
    "key:generate": "php artisan key:generate && php artisan key:generate --env=testing",
    "jwt:generate": "php artisan jwt:secret && php artisan jwt:secret --env=testing",
    "permissions": "chgrp -R www-data storage bootstrap/cache && chmod -R ug+rwx storage bootstrap/cache"
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
